<!--This workflow is only for testing compress chains.-->
<workflow-app name='compress-chains' xmlns="uri:oozie:workflow:0.1">
    <start to='CompressChains'/>
    <action name='CompressChains'>
        <java>
            <job-tracker>${jobTracker}</job-tracker>
            <name-node>${nameNode}</name-node>
	    <job-xml>${stageConfigDir}/CompressChains.xml</job-xml>	    
<configuration>
                <property>
                    <name>mapred.queue.name</name>
                    <value>default</value>
                </property>
</configuration>
	    <main-class>contrail.avro.CompressChains</main-class>
	    <arg>inputpath=${outputDir}/QuickMerge</arg>
	    <arg>outputpath=${outputDir}/CompressChains</arg>
	    <capture-output/>
        </java>
        <ok to='end'/>
        <error to='kill'/>
    </action>
    <kill name="kill">
        <message>Map/Reduce failed, error message[${wf:errorMessage()}]</message>
    </kill>
    <end name='end'/>
</workflow-app>
